{
  "name": "cookify-app",
  "version": "1.0.0",
  "description": "Cookify food delivery application",
  "main": "Cookify Backend/server.js",
  "scripts": {
    "start": "./start.sh",
    "build": "./build.sh",
    "postinstall": "cd \"Cookify Backend\" && npm install",
